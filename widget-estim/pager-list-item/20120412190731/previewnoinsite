<script language="javascript" type="text/javascript">
function shareContent(urlSharer) {
var targetURL = "http://webprep.dmzprive.cite-sciences.fr";
var loc = "";
if( targetURL && targetURL.length > 0 ) {
loc = targetURL;
}
var apos = loc.indexOf("#");
loc = (apos > 0 ? loc.substring(0, apos) : loc);
var urlLien = urlSharer + encodeURIComponent(loc) + "&title=" + encodeURIComponent("null");
window.open(urlLien);
}
</script>

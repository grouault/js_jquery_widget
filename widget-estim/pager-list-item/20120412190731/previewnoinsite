<script language="javascript" type="text/javascript">
function mailpage() {
chaine_mail = "mailto:?subject=" + escape('Expositions / Visites') + " : ";
chaine_mail += '&body=' + escape('http://webprep.dmzprive.cite-sciences.fr/fr/a-la-une-des-regions/contenu-acs/wl/1248102503665/expositions-visites/rendermode/previewnoinsite');
window.location.href = chaine_mail;
}
</script>
<script type="text/javascript" language="javascript">
var currentFontSize = 0;
var checkclick = 0;
$(document).ready(function() {
var temp = "";
// Increase Font Size
$('.increaseFont').click(function() {
currentFontSize = $('a').css('font-size');
if (checkclick ==0){
currentFontSize = 11;
}
var currentFontSizeNum = parseFloat(currentFontSize, 10);
var newFontSize = currentFontSizeNum+1;
if( newFontSize < 16 ) {
$('a').css('font-size', newFontSize);
$('div').css('font-size', newFontSize);
$('p').css('font-size', newFontSize);
$('li').css('font-size', newFontSize);
$('span').css('font-size', newFontSize);
$('input').css('font-size', newFontSize);
$('.content').css('font-size', newFontSize);
}
checkclick = 1;
return false;
});
// Decrease Font Size
$(".decreaseFont").click(function() {
currentFontSize = $('a').css('font-size');
if( checkclick == 0 ) {
currentFontSize = 11;
}
var currentFontSizeNum = parseFloat(currentFontSize, 10);
var newFontSize = currentFontSizeNum-1;
if( newFontSize > 8 ) {
$('a').css('font-size', newFontSize);
$('div').css('font-size', newFontSize);
$('p').css('font-size', newFontSize);
$('li').css('font-size', newFontSize);
$('span').css('font-size', newFontSize);
$('input').css('font-size', newFontSize);
$('.content').css('font-size', newFontSize);
}
checkclick = 1;
return false;
});
});
$(document).ready(function(){
var listeLi = document.getElementById("breadcrumb").getElementsByTagName("li");
var tampon = "";
var lastElem = listeLi.length -1;
if( listeLi.length > 4 ) {
for( var i=0; i < listeLi.length; i++ ) {
if( listeLi[i].getElementsByTagName("a")[0] == null ) {
tampon = tampon + listeLi[i].innerHTML.replace(/^\s+|\s+$/g,"");
} else {
tampon = tampon + listeLi[i].getElementsByTagName("a")[0].innerHTML.replace(/^\s+|\s+$/g,"");
}
}
if( tampon.length > 125 ) {
document.getElementById("breadcrumb").getElementsByTagName("li")[1].innerHTML="  ... > ";
document.getElementById("breadcrumb").getElementsByTagName("li")[2].style.display ='none';
document.getElementById("breadcrumb").getElementsByTagName("li")[3].style.display ='none';
document.getElementById("breadcrumb").getElementsByTagName("li")[4].style.display ='none';
}
if( tampon.length > 105 ) {
document.getElementById("breadcrumb").getElementsByTagName("li")[1].innerHTML="  ... > ";
document.getElementById("breadcrumb").getElementsByTagName("li")[2].style.display ='none';
document.getElementById("breadcrumb").getElementsByTagName("li")[3].style.display ='none';
} else if( tampon.length > 85 ) {
document.getElementById("breadcrumb").getElementsByTagName("li")[1].innerHTML="  ... > ";
document.getElementById("breadcrumb").getElementsByTagName("li")[2].style.display ='none';
}
}
});
</script>
